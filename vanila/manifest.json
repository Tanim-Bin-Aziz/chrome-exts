{
  "manifest_version": 3,
  "name": "Screenshot Extension",
  "version": "1.0.0",
  "description": "Take a screenshot of the current page.",
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "48": "logo1.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"]
    }
  ]
}
